# AuthenticationExample

use_frameworks!
platform :ios, '8.0'
# [START auth_pod]
pod 'Firebase/Auth'
pod 'Firebase/Core'
# [END auth_pod]

# These are pods used for the auth providers.
pod 'FBSDKLoginKit'
# [START google_pod]
pod 'GoogleSignIn'
# [END google_pod]
pod 'TwitterKit'
pod 'SVProgressHUD'

target 'ProjectIOS' do
end
